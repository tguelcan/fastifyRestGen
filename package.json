{
  "name": "restgen",
  "version": "0.0.1",
  "description": "fastify generator",
  "main": "src/server.js",
  "scripts": {
    "test": "babel-tape-runner \"src/**/*.test.js\" | faucet",
    "posttest": "tape --coverage-report=text-lcov | codecov",
    "dev": "nodemon --exec babel-node src/server.js",
    "build": "babel src -d lib",
    "serve": "node dist/server.js"
  },
  "keywords": [
    "fastify",
    "generator",
    "restful",
    "rest"
  ],
  "author": "Tayfun Guelcan",
  "license": "MIT",
  "dependencies": {
    "fastify": "^2.12.1",
    "fastify-autoload": "^1.2.2",
    "fastify-blipp": "^2.1.0",
    "fastify-env": "^1.0.1",
    "fastify-helmet": "^3.0.2",
    "fastify-oas": "^2.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-arrow-functions": "^7.8.3",
    "@babel/plugin-transform-parameters": "^7.8.4",
    "@babel/preset-env": "^7.8.6",
    "babel-eslint": "^10.1.0",
    "babel-plugin-root-import": "^6.4.1",
    "babel-tape-runner": "^3.0.0",
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "faucet": "^0.0.1",
    "nodemon": "^2.0.2",
    "plop": "^2.5.4",
    "tape": "^4.13.0"
  }
}
