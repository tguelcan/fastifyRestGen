{
  "name": "restgen",
  "version": "0.0.1",
  "description": "fastify generator",
  "main": "lib/server.js",
  "author": "Tayfun Guelcan",
  "scripts": {
    "test": "cross-env NODE_ENV=test babel-tape-runner \"src/**/*.test.js\" | faucet",
    "coverage": "nyc --reporter=html --reporter=text-summary npm run test",
    "dev": "nodemon --exec babel-node src/server.js",
    "build": "babel src -d lib --ignore \"src/**/*.test.js\"",
    "serve": "node dist/server.js"
  },
  "keywords": [
    "fastify",
    "generator",
    "restful",
    "rest"
  ],
  "license": "MIT",
  "dependencies": {
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "fastify": "^2.12.1",
    "fastify-autoload": "^1.2.2",
    "fastify-blipp": "^2.1.0",
    "fastify-env": "^1.0.1",
    "fastify-formbody": "^3.1.0",
    "fastify-helmet": "^3.0.2",
    "fastify-oas": "^2.5.2",
    "fastify-rate-limit": "^3.0.1",
    "fastify-sensible": "^2.1.1",
    "fastify-sentry": "^1.4.0",
    "lodash": "^4.17.15",
    "mongodb-memory-server": "^6.5.0",
    "mongoose": "^5.9.6",
    "mongoose-query-parser": "^1.1.5",
    "pino-pretty": "^3.6.0",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-arrow-functions": "^7.8.3",
    "@babel/plugin-transform-parameters": "^7.8.4",
    "@babel/preset-env": "^7.8.6",
    "babel-eslint": "^10.1.0",
    "babel-plugin-root-import": "^6.4.1",
    "babel-tape-runner": "^3.0.0",
    "eslint": "^6.8.0",
    "faucet": "^0.0.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "plop": "^2.5.4",
    "tape": "^4.13.0"
  }
}
